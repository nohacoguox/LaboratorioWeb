# Copy this file to .env and fill values for production on Render.
NODE_ENV=production
# Render injects PORT automatically
# PORT=10000

# External Database URL from Render (PostgreSQL)
DATABASE_URL=postgres://USER:PASSWORD@HOST:PORT/DBNAME

# Used to sign JWT tokens
JWT_SECRET=change_this_super_secret_key

# Leave empty when serving frontend from same origin
CORS_ORIGIN=
